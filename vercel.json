{
  "version":2,
  "builds":[
    {
      "src":"./backend/index.js",
      "use":"@vercel/node"
    }
  ],
  "routes":[
    {
      "src":"./aviaFrontend/dist",
      "dest":"@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "./backend/server.js"
    }
  ]
}